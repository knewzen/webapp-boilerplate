{
  "private": true,
  "name": "webapp-boilerplate",
  "version": "0.4.0",
  "description": "Scaffold out a front-end web app using Gulp and Browserify",
  "license": "MIT @ Radu Micu",
  "author": "Radu Micu",
  "engines": {
    "node": ">=4",
    "npm": ">=1.3"
  },
  "files": [
    "app"
  ],
  "keywords": [
    "web",
    "app",
    "front-end",
    "h5bp",
    "modernizr",
    "jquery",
    "gulp",
    "browserify",
    "rollupify"
  ],
  "scripts": {
    "start": "gulp serve",
    "test": "ava --verbose",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.14.3",
    "browserify": "^13.1.0",
    "chalk": "^1.1.3",
    "del": "^2.2.2",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-formatter-pretty": "^0.3.1",
    "eslint-plugin-import": "^1.14.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.4.5",
    "gulp-cssnano": "^2.1.2",
    "gulp-duration": "0.0.0",
    "gulp-eslint": "^3.0.1",
    "gulp-htmlmin": "^2.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.3",
    "gulp-load-plugins": "^1.2.4",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylelint": "^3.1.0",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.1.1",
    "gulp-util": "^3.0.7",
    "jsdom": "^9.4.5",
    "main-bower-files": "^2.13.1",
    "nyc": "^8.1.0",
    "postcss-scss": "^0.2.1",
    "rollupify": "^0.3.4",
    "sinon": "^1.17.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "wiredep": "^4.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ],
    "source": [
      "app/scripts/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit",
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "nyc": {
    "include": [
      "app/scripts/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  }
}
